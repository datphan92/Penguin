<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Item">
  <TemplateData>
    <Name>3. [Business] Interface</Name>
    <Description>Create an interface based on IDWF Business Interface</Description>
    <ProjectType>CSharp</ProjectType>
    <ShowByDefault>true</ShowByDefault>
    <DefaultName>NameOfInterface</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <SortOrder>1</SortOrder>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <ProjectItem TargetFileName="I$fileinputname$Business.cs" ReplaceParameters="true">BusinessInterface.cs</ProjectItem>
    <CustomParameters>
      <CustomParameter Name="$solutionname$" Value="IDWF"/>
    </CustomParameters>
  </TemplateContent>
</VSTemplate>